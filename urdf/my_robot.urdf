<?xml version="1.0"?>
<!-- URDF WORKS TILL wrist. Need to fix positioning and then work on gripper -->
<robot name="testing-robot">

  <link name="world"></link>

  <joint name="base_joint" type="fixed">
      <origin xyz="1 1.5 0" rpy="0 0 0"/>
      <parent link="world"/>
      <child link="base_link"/>
      
  </joint>

  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <origin xyz="-0.04 0.092785522699356109966 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Base.dae" scale="1 1 1"/>
      </geometry>
      <!-- <geometry>
        <box size="0.2 0.2 0.2"/>
      </geometry> -->

    </visual>
    <collision>
      <origin xyz="-0.04 0.092785522699356109966 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Base.dae" scale="1 1 1"/>
      </geometry>
      <!-- <geometry>
        <box size="0.2 0.2 0.2"/>
      </geometry> -->

    </collision>
  </link>

  <!-- Revolute joint connecting base_link to central_axis_link -->
  <joint name="joint_base_to_central" type="revolute">
    <origin xyz="0 0 0.093107574462890613232"/>
    <parent link="base_link"/>
    <child link="central_axis_link"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="3.14" effort="100" velocity="100"/>
    
    
    
    
  </joint>

  <!-- Central Axis Link -->
  <link name="central_axis_link">
    <visual>
      <origin xyz="-0.04 0.092785522699356109966 -0.093107574462890613232" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/EjeCentral.dae" scale="1 1 1"/>
      </geometry>
      <!-- <geometry>
        <box size="0.2 0.2 0.2"/>
      </geometry> -->

    </visual>
    <collision>
      <origin xyz="-0.04 0.092785522699356109966 -0.093107574462890613232" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/EjeCentral.dae" scale="1 1 1"/>
      </geometry>
      <!-- <geometry>
        <box size="0.2 0.2 0.2"/>
      </geometry> -->

    </collision>
  </link>

  <!-- Revolute joint: central_axis_link to arm_link -->
  <joint name="joint_central_to_forearm" type="revolute">
    <parent link="central_axis_link"/>
    <child link="forearm_link"/>
    <origin xyz="0 0 0.07" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
  </joint>

  <!-- Forearm Link -->
  <link name="forearm_link">
    <visual>
      <origin xyz="-0.028429296970367431974 0.080424407958984450717 -0.16" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Antebrazo.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.028429296970367431974 0.080424407958984450717 -0.16" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Antebrazo.dae" scale="1 1 1"/>
      </geometry>
    </collision> 
  </link>

  <!-- Revolute joint: arm_link to forearm_link -->
  <joint name="joint_forearm_to_arm" type="revolute">
    <parent link="forearm_link"/>
    <child link="arm_link"/>
    <origin xyz="0 0 0.13" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
  </joint>

  <!-- Arm Link -->
  <link name="arm_link">
    <visual>
      <origin xyz="-0.02 0.08633435821533209931 -0.29242617797851566674" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Brazo.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.02 0.08633435821533209931 -0.29242617797851566674" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Brazo.dae" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Revolute joint: forearm_link to wrist_link -->
  <joint name="joint_arm_to_wrist" type="revolute">
    <parent link="arm_link"/>
    <child link="wrist_link"/>
    <origin xyz="-0.103129016876 -0.011158664703 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
  </joint>

  <!-- Wrist Link -->
  <link name="wrist_link">
    <visual>
      <origin xyz="0.078422674179077153656 0.097 -0.305" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Muteca.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.078422674179077153656 0.097 -0.305" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Muteca.dae" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Revolute joint: wrist_link to gripper_base_link -->
  <joint name="joint_wrist_to_gripper_base" type="revolute">
    <parent link="wrist_link"/>
    <child link="gripper_base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
  </joint>

  <!-- Gripper Base Link -->
  <link name="gripper_base_link">
    <visual>
      <origin xyz="-0.023109045028686523576 0.021828266918659258722 -0.19234000396728520577" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Basedelagarra.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.023109045028686523576 0.021828266918659258722 -0.19234000396728520577" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Basedelagarra.dae" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Revolute joint: gripper_base_link to gripper_finger1_link -->
  <joint name="joint_gripper_finger1" type="revolute">
    <parent link="gripper_base_link"/>
    <child link="gripper_finger1_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="3.14" effort="5" velocity="1"/>
  </joint>

  <!-- Gripper finger 1 -->
  <link name="gripper_finger1_link">
    <visual>
      <origin xyz="0 0 -0.19234000396728520577" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Pinza1.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.19234000396728520577" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Pinza1.dae" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Revolute joint: gripper_base_link to gripper_finger2_link -->
  <joint name="joint_gripper_finger2" type="fixed">
    <parent link="gripper_finger1_link"/>
    <child link="gripper_finger2_link"/>
    <!-- <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="3.14" effort="5" velocity="1"/> -->
  </joint>

  <!-- Gripper finger 2 -->
  <link name="gripper_finger2_link">
    <visual>
      <origin xyz="0 0 -0.19234000396728520577" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Pinza2.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.19234000396728520577" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_rviz/meshes/Pinza2.dae" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

</robot>
